{
  "project_name": "nri-redis",
  "kind_cluster_name": "nri-redis-dev",
  "image_name": "nri-redis-dev",
  "live_reload": true,
  "chart_path": "devel/charts/integration",
  "default_registry": "localhost:50101",
  "port_forwards": [
    "50100:50100"
  ],
  "entrypoint":[
    "dlv",
    "--listen=0.0.0.0:50100",
    "--headless=true",
    "--api-version=2",
    "--check-go-version=false",
    "--only-same-user=false",
    "--accept-multiclient",
    "exec",
    "/var/db/newrelic-infra/newrelic-integrations/bin/nri-redis",
    "--"
  ]
}